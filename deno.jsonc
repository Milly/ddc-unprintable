{
  "name": "@milly/ddc-unprintable",
  "exports": {
    ".": "./lib.ts",
    "./error": "./error.ts",
    "./type": "./type.ts"
  },
  "publish": {
    "include": [
      "LICENSE",
      "README.md",
      "**/*.ts"
    ],
    "exclude": [
      "**/*_test.ts"
    ]
  },
  "lock": false,
  "tasks": {
    "cache": "deno install",
    "check": "deno task check:lint && deno task check:fmt && deno task check:type && deno task check:doc",
    "check:lint": "deno lint",
    "check:fmt": "deno fmt --check",
    "check:type": "deno check",
    "check:doc": "deno check --doc-only **/*.ts **/*.md",
    "check:publish": "deno publish --dry-run --set-version 0.0.0",
    "fix": "deno lint --fix && deno fmt",
    "test": "LANG=C deno test -A --doc --parallel --shuffle",
    "test:coverage": "deno task test --coverage=.coverage",
    "coverage": "deno coverage .coverage",
    "coverage:clean": "rm -rf .coverage"
  },
  "imports": {
    "@core/unknownutil": "jsr:@core/unknownutil@4",
    "@denops/std": "jsr:@denops/std@^8.1.1",
    "@milly/denops-batch-accumulate": "jsr:@milly/denops-batch-accumulate@^3.0.1",
    "@shougo/ddc-vim": "jsr:@shougo/ddc-vim@10",
    // Imports for `--doc` test.
    "jsr:@milly/ddc-unprintable": "./lib.ts",
    "jsr:@milly/ddc-unprintable/type": "./type.ts",
    "jsr:@shougo/ddc-vim": "jsr:@shougo/ddc-vim@10"
  }
}
